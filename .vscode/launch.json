    "version": "0.2.0",
    "configurations": [
        {
            "name": "tree-disk1",
            "type": "debugpy",
            "request": "launch",
            "module": "treediskpithdetector.cli",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "--filename",
                "./input/tree-disk1.jpg",
                "--output_dir",
                "output/",
                "--new_shape",
                "640",
                "--method",
                "apd_dl",
                "--weights_path",
                "./models/all_best_yolov8.pt",
                "--debug"
            ],
            "console": "integratedTerminal"
        },
